{
    "name": "@thi.ng/geom-accel",
    "version": "1.2.10",
    "description": "n-D spatial indexing data structures",
    "module": "./index.js",
    "main": "./lib/index.js",
    "umd:main": "./lib/index.umd.js",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/master/packages/geom-accel",
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && yarn build:es6 && node ../../scripts/bundle-module",
        "build:release": "yarn clean && yarn build:es6 && node ../../scripts/bundle-module all",
        "build:es6": "tsc --declaration",
        "build:test": "rimraf build && tsc -p test/tsconfig.json",
        "test": "mocha test",
        "cover": "nyc mocha test && nyc report --reporter=lcov",
        "clean": "rimraf *.js *.d.ts .nyc_output build coverage doc lib internal",
        "doc:readme": "../../scripts/generate-readme",
        "doc": "node_modules/.bin/typedoc --mode modules --out doc src",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && node_modules/.bin/api-extractor run --local --verbose",
        "pub": "yarn build:release && yarn publish --access public"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^0.1.3",
        "@microsoft/api-extractor": "^7.7.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.12.11",
        "mocha": "^6.2.2",
        "nyc": "^14.0.0",
        "ts-node": "^8.5.2",
        "typedoc": "^0.15.2",
        "typescript": "^3.7.2"
    },
    "dependencies": {
        "@thi.ng/api": "^6.6.0",
        "@thi.ng/arrays": "^0.4.0",
        "@thi.ng/equiv": "^1.0.11",
        "@thi.ng/geom-api": "^0.3.8",
        "@thi.ng/geom-isec": "^0.3.10",
        "@thi.ng/heaps": "^1.1.6",
        "@thi.ng/math": "^1.5.1",
        "@thi.ng/transducers": "^6.1.0",
        "@thi.ng/vectors": "^4.0.1"
    },
    "keywords": [
        "2D",
        "3D",
        "nD",
        "data structure",
        "ES6",
        "acceleration",
        "kd-tree",
        "pointcloud",
        "spatial index",
        "tree",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "thi.ng": {
        "year": 2013
    }
}
