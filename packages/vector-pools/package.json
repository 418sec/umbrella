{
    "name": "@thi.ng/vector-pools",
    "version": "1.0.40",
    "description": "Data structures for managing & working with strided, memory mapped vectors",
    "module": "./index.js",
    "main": "./lib/index.js",
    "umd:main": "./lib/index.umd.js",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/vector-pool#readme",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/postspectacular"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/thing_umbrella"
        }
    ],
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && yarn build:es6 && node ../../scripts/bundle-module",
        "build:release": "yarn clean && yarn build:es6 && node ../../scripts/bundle-module all",
        "build:es6": "tsc --declaration",
        "build:test": "rimraf build && tsc -p test/tsconfig.json",
        "build:check": "tsc --isolatedModules --noEmit",
        "test": "mocha test",
        "cover": "nyc mocha test && nyc report --reporter=lcov",
        "clean": "rimraf *.js *.d.ts .nyc_output build coverage doc lib",
        "doc:readme": "ts-node -P ../../tools/tsconfig.json ../../tools/src/readme.ts",
        "doc": "node_modules/.bin/typedoc --mode modules --out doc src",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && node_modules/.bin/api-extractor run --local --verbose",
        "pub": "yarn build:release && yarn publish --access public"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@microsoft/api-extractor": "^7.9.2",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.26",
        "mocha": "^8.0.1",
        "nyc": "^15.1.0",
        "ts-node": "^8.10.1",
        "typedoc": "^0.17.6",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "@thi.ng/api": "^6.12.0",
        "@thi.ng/binary": "^2.0.11",
        "@thi.ng/checks": "^2.7.4",
        "@thi.ng/malloc": "^4.1.21",
        "@thi.ng/transducers": "^7.2.0",
        "@thi.ng/vectors": "^4.6.0"
    },
    "files": [
        "*.js",
        "*.d.ts",
        "lib"
    ],
    "keywords": [
        "ES6",
        "memory",
        "memory mapped",
        "pool",
        "typedarray",
        "typescript",
        "webgl",
        "wasm"
    ],
    "publishConfig": {
        "access": "public"
    },
    "browser": {
        "process": false,
        "setTimeout": false
    },
    "sideEffects": false,
    "thi.ng": {
        "related": [
            "ecs",
            "malloc",
            "soa",
            "unionstruct",
            "vectors",
            "webgl"
        ],
        "status": "alpha",
        "year": 2018
    }
}
