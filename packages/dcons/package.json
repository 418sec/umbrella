        "@thi.ng/compare": "^0.1.11",
        "@thi.ng/equiv": "^0.1.13",
        "@thi.ng/errors": "^0.1.11",
        "@thi.ng/transducers": "^2.2.2"
    },
    "keywords": [
        "datastructure",
        "doubly linked",
        "ES6",
        "iterators",
        "linkedlist",
