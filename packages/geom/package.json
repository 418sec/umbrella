{
    "name": "@thi.ng/geom",
    "version": "1.10.6",
    "description": "Functional, polymorphic API for 2D geometry types & SVG generation",
    "module": "./index.js",
    "main": "./lib/index.js",
    "umd:main": "./lib/index.umd.js",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/geom#readme",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/postspectacular"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/thing_umbrella"
        }
    ],
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && yarn build:es6 && node ../../scripts/bundle-module",
        "build:release": "yarn clean && yarn build:es6 && node ../../scripts/bundle-module all",
        "build:es6": "tsc --declaration",
        "build:test": "rimraf build && tsc -p test/tsconfig.json",
        "build:check": "tsc --isolatedModules --noEmit",
        "test": "mocha test",
        "cover": "nyc mocha test && nyc report --reporter=lcov",
        "clean": "rimraf *.js *.d.ts .nyc_output build coverage doc lib ctors internal ops",
        "doc:readme": "ts-node -P ../../tools/tsconfig.json ../../tools/src/readme.ts",
        "doc": "node_modules/.bin/typedoc --mode modules --out doc src",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && node_modules/.bin/api-extractor run --local --verbose",
        "pub": "yarn build:release && yarn publish --access public"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@microsoft/api-extractor": "^7.8.0",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.0.1",
        "mocha": "^8.0.1",
        "nyc": "^15.1.0",
        "ts-node": "^8.10.1",
        "typedoc": "^0.17.6",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "@thi.ng/api": "^6.11.3",
        "@thi.ng/arrays": "^0.6.12",
        "@thi.ng/checks": "^2.7.3",
        "@thi.ng/compose": "^1.4.12",
        "@thi.ng/defmulti": "^1.2.20",
        "@thi.ng/equiv": "^1.0.26",
        "@thi.ng/errors": "^1.2.16",
        "@thi.ng/geom-api": "^1.0.25",
        "@thi.ng/geom-arc": "^0.3.3",
        "@thi.ng/geom-clip-line": "^1.1.3",
        "@thi.ng/geom-clip-poly": "^1.0.23",
        "@thi.ng/geom-closest-point": "^0.3.36",
        "@thi.ng/geom-hull": "^0.0.56",
        "@thi.ng/geom-isec": "^0.4.25",
        "@thi.ng/geom-poly-utils": "^0.1.54",
        "@thi.ng/geom-resample": "^0.2.36",
        "@thi.ng/geom-splines": "^0.5.23",
        "@thi.ng/geom-subdiv-curve": "^0.1.53",
        "@thi.ng/geom-tessellate": "^0.2.36",
        "@thi.ng/hiccup": "^3.5.3",
        "@thi.ng/hiccup-svg": "^3.5.3",
        "@thi.ng/math": "^1.7.13",
        "@thi.ng/matrices": "^0.6.23",
        "@thi.ng/random": "^1.4.14",
        "@thi.ng/transducers": "^7.1.4",
        "@thi.ng/vectors": "^4.5.3"
    },
    "files": [
        "*.js",
        "*.d.ts",
        "lib",
        "api",
        "ctors",
        "internal",
        "ops"
    ],
    "keywords": [
        "2D",
        "ES6",
        "conversion",
        "intersection",
        "geometry",
        "shapes",
        "SVG",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "thi.ng": {
        "shortlink": false,
        "status": "beta",
        "year": 2013
    }
}
